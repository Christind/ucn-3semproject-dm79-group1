<?xml version="1.0" encoding="utf-8" ?><configuration>  <connectionStrings>    <!--  you know what to do here.... -->  </connectionStrings>  <appSettings>    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />  </appSettings>  <system.web>    <compilation debug="true" targetFramework="4.5" />    <httpRuntime targetFramework="4.5"/>  </system.web>  <system.serviceModel>    <services>      <service name="RestfulAPI.APICollection">        <endpoint address="" binding="webHttpBinding" behaviorConfiguration="REST" contract="RestfulAPI.IAPICollection" />      </service>    </services>    <behaviors>      <endpointBehaviors>        <behavior name="REST">          <webHttp />        </behavior>      </endpointBehaviors>      <serviceBehaviors>        <behavior>          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>          <serviceDebug includeExceptionDetailInFaults="false"/>        </behavior>      </serviceBehaviors>    </behaviors>    <protocolMapping>      <add binding="basicHttpsBinding" scheme="https" />    </protocolMapping>    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />  </system.serviceModel>  <system.webServer>    <modules runAllManagedModulesForAllRequests="true"/>    <directoryBrowse enabled="true"/>  </system.webServer></configuration>